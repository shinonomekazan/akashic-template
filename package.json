{
	"name": "akashic-template",
	"version": "0.1.0",
	"description": "",
	"scripts": {
		"clean": "rimraf ./script",
		"watch": "webpack -w",
		"build": "webpack",
		"build:production": "NODE_ENV=production webpack",
		"update": "akashic-cli-scan asset && akashic-cli-scan globalScripts",
		"preexport:content": "npm run build:production",
		"prettier:check": "prettier --check .",
		"prettier:write": "prettier --write .",
		"lint": "npm run prettier:check",
		"lint:fix": "npm run prettier:write",
		"start": "akashic-sandbox .",
		"serve": "akashic-cli-serve --no-open-browser",
		"export:zip": "rimraf game.zip && akashic-cli-export-zip -o game.zip -s",
		"export:html": "rimraf game && akashic-cli-export-html -o game",
		"export:content": "rimraf game.zip && akashic-cli-export-html -o ./game.zip --atsumaru",
		"test": "npm run lint && jest"
	},
	"author": "Shinonomekazan Co., Ltd.",
	"license": "MIT",
	"devDependencies": {
		"@akashic/akashic-cli-export-html": "1.2.1",
		"@akashic/akashic-cli-export-zip": "0.7.1",
		"@akashic/akashic-cli-scan": "0.6.1",
		"@akashic/akashic-cli-serve": "^1.2.2",
		"@akashic/akashic-engine": "~3.0.2",
		"@akashic/akashic-sandbox": "~0.17.4",
		"@akashic/headless-akashic": "^1.0.0",
		"@types/jest": "26.0.19",
		"@types/node": "14.14.20",
		"jest": "26.6.3",
		"prettier": "^2.2.1",
		"rimraf": "3.0.2",
		"ts-jest": "26.4.4",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"webpack": "^5.12.1",
		"webpack-cli": "^4.3.1"
	}
}
